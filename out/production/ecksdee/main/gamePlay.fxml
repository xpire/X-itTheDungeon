<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<Group xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.GamePlayController">
   <children>
      <Label layoutY="500.0" text="Play Mode">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="Exit" layoutX="700.0" layoutY="10.0" mnemonicParsing="false" onAction="#GoToMainMenu" text="Exit" />
   </children>
</Group>
